{
    "name": "teqneers/ext-application-bundle",
    "type": "library",
    "description": "A Symfony 2 bundle to integrate Sencha Ext JS into a Symfony 2 application",
    "homepage": "https://github.com/teqneers/ext-application-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "http://opensource.teqneers.de/"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\Bundle\\ExtJSApplicationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\Bundle\\ExtJSApplicationBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/http-kernel": "~2.6|~3.0.0",
        "symfony/config": "~2.6|~3.0.0",
        "symfony/dependency-injection": "~2.6|~3.0.0",
        "symfony/routing": "~2.6|~3.0.0",
        "teqneers/ext-application": "dev-master",
        "twig/twig": "~1.18"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "symfony/framework-bundle": "~2.6|~3.0.0"
    }
}
